# Google Tag Manager Container ID
# Get your container ID at: https://tagmanager.google.com/
VITE_GTM_CONTAINER_ID=GTM-XXXXXXX

# Dune API Key (free tier available)
# Get your API key at: https://dune.com/settings/api
VITE_DUNE_API_KEY=your_api_key_here

# Dune Query IDs for stablecoin metrics
# Create these queries in Dune or use existing community queries
# Query should return columns: date, transfer_volume (in USD)
VITE_DUNE_TRANSFER_VOLUME_QUERY_ID=your_query_id_here
# Query should return columns: date, transaction_count
VITE_DUNE_TRANSACTION_COUNT_QUERY_ID=your_query_id_here
# Query should return columns: date, active_addresses
VITE_DUNE_ACTIVE_ADDRESSES_QUERY_ID=your_query_id_here
